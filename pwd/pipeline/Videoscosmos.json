{
	"name": "Videoscosmos",
	"properties": {
		"activities": [
			{
				"name": "Copy_cgq",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "movies"
					},
					{
						"name": "Destination",
						"value": "rawfilesystem/AJTest/GP/P/C/GC/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DocumentDbCollectionSource",
						"nestingSeparator": "."
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_cgq",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_cgq",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}